{
    "name": "islamdb/orchid-setting",
    "description": "Setting in orchid platform",
    "keywords": [
        "orchid",
        "setting"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "islamdb",
            "email": "islamdbrnd@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "orchid/platform": "^10.0|^11.0|^12.0",
        "islamdb/orchid-helper": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "IslamDB\\OrchidSetting\\OrchidSettingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IslamDB\\OrchidSetting\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
